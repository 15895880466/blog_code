<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.mundane.leakdemo">

  <application
      android:name=".app.LeakDemoApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity android:name=".LeakStaticActivity">
    </activity>
    <activity android:name=".AsyncTaskActivity">
    </activity>
    <activity android:name=".SingletonLeakActivity">
    </activity>
    <activity android:name=".StaticViewLeakActivity">
    </activity>
    <activity android:name=".HandlerLeakActivity">
    </activity>
    <activity android:name=".RunnableLeakActivity">
    </activity>
  </application>

</manifest>